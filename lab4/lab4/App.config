  <userSettings>
    <lab4.Properties.Settings>
      <setting name="RemoveNewsAfterInDays" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="NewsAutoRefreshInHours" serializeAs="String">
        <value>6</value>
      </setting>
      <setting name="RefreshNewsOnStartup" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="Theme" serializeAs="String">
        <value>DeepPurple</value>
      </setting>
